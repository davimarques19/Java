package entities;

import java.util.ArrayList;
import java.util.Date;
import java.util.List;

public class Enviar {

	private Date momento;
	private String titulo;
	private String contem;
	private Integer likes;
	
	private List<Comentario> com = new ArrayList<>();

	public Enviar(Date momento, String titulo, String contem, Integer likes) {
		this.momento = momento;
		this.titulo = titulo;
		this.contem = contem;
		this.likes = likes;
	}

	public Date getMomento() {
		return momento;
	}

	public void setMomento(Date momento) {
		this.momento = momento;
	}

	public String getTitulo() {
		return titulo;
	}

	public void setTitulo(String titulo) {
		this.titulo = titulo;
	}

	public String getContem() {
		return contem;
	}

	public void setContem(String contem) {
		this.contem = contem;
	}

	public Integer getLikes() {
		return likes;
	}

	public void setLikes(Integer likes) {
		this.likes = likes;
	}

	public List<Comentario> getCom() {
		return com;
	}
	
	
}
